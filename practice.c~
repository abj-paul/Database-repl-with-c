#include<stdio.h>
#include<stdlib.h>
#include<stdbool.h>

typedef struct {
  char* buffer;
  size_t buffer_legth;
  ssize_t input_length;
}InputBuffer;

InputBuffer* new_input_buffer(){
  InputBuffer* input_buffer = (InputBuffer*)malloc(sizeof(InputBuffer));
}

int main(){

  while(true){
    InputBuffer* input_buffer = new_input_buffer();
  }

  return 0;
}
